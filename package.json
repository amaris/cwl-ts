{
  "name": "cwlts-dev",
  "version": "1.0.0",
  "description": "TypeScript data model for Common Workflow Language",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "test": "./node_modules/typescript/bin/tsc ./src/**/*.ts --noEmitHelpers --lib es6 --target es5 && mocha ./**/*.spec.js",
    "coverage": "npm install istanbul && tsc /src/**/*.ts --lib es6 --target es5 && ./node_modules/istanbul/lib/cli.js cover _mocha ./**/*.spec.js",
    "regenerate": "./node_modules/cwl-ts-parser/src/bin/generate.js ./src/mappings",
    "schemas": "npm install cwl-ts-parser && scripts/schemas.sh",
    "d2sb-test": "mocha src/tests/*.d2sb.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabix/cwl-ts.git"
  },
  "author": "Ivan BatiÄ‡",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rabix/cwl-ts/issues"
  },
  "homepage": "https://github.com/rabix/cwl-ts#readme",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "chai": "^3.5.0",
    "jsonschema": "^1.1.0",
    "mocha": "^2.5.3",
    "typescript": "^2.0.0-beta",
    "typescript-json-schema": "0.1.1"
  }
}
